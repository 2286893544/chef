{
  "name": "webserve",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "3": "pm2 stop webserve",
    "start": "cross-env NODE_ENV=production pm2 start ./bin/www --name webserve",
    "dev": "cross-env NODE_ENV=development nodemon --watch ./ --ext js,ts,json --ignore ./node_modules ./bin/www",
    "prod-restart": "pm2 restart webserve",
    "prod-delete": "pm2 delete webserve",
    "logs": "pm2 logs webserve"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "md5": "^2.3.0",
    "mongoose": "^8.6.0",
    "morgan": "~1.9.1",
    "multiparty": "^4.2.3",
    "node-schedule": "^2.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7"
  }
}
